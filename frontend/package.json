{
  "name": "heatstrike-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && tsc -b --noEmit",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -b --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "preview": "vite preview",
    "generate:api": "openapi-ts -i ../backend/openapi.json -o ./src/lib/api/generated"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.158.4",
    "@visx/axis": "^3.12.0",
    "@visx/event": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/heatmap": "^3.12.0",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/tooltip": "^3.12.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@hey-api/openapi-ts": "^0.92.3",
    "@tanstack/router-plugin": "^1.158.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.5.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
